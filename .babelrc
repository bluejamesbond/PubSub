{
  "env":{
    "impl":{
      "presets":[
        "node5",
        "stage-0"
      ],
      "plugins":[
        "array-includes",
        "transform-object-rest-spread",
        "syntax-class-properties",
        [
          "syntax-async-functions",
          {
            "module":"bluebird"
          }
        ]
      ]
    },
    "dist":{
      "presets":[
        "es2015",
        "stage-0"
      ],
      "plugins":[
        "array-includes",
        "transform-object-rest-spread",
        "syntax-class-properties",
        [
          "syntax-async-functions",
          {
            "module":"bluebird"
          }
        ]
      ]
    }
  }
}